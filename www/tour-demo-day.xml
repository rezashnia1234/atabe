<krpano onstart="startup();" showerrors="true" logkey="true">

	<action name="startup">
		loadscene("eyvan", null, MERGE);
	</action>
	
	<plugin name="gyro" devices="html5" keep="true"
			url="%SWFPATH%/plugins/gyro2.js" 
			enabled="true"
			camroll="true"
			friction="0.0"
			touch_mode="full"
			sensor_mode="1"
			autocalibration="true"
			onavailable=""
			onunavailable=""
			onenable=""
			ondisable=""
			/>
	 
	<scene name="eyvan" title="object" thumburl="panos/eyvan-day/thumb.jpg" >
		<preview url="panos/eyvan-day/preview.jpg" />
		<image >
			<cube url="panos/eyvan-day/mobile_%s.jpg" />
		</image>
	</scene>

</krpano>