<krpano onstart="startup();" showerrors="true" logkey="true">

	<action name="startup">
		loadscene("eyvan", null, MERGE);
	</action>
	
    <plugin
		name="gyro"
		devices="html5"
		url="%SWFPATH%/plugins/gyro2.js"
		keep="true"
		camroll="true" friction="0" velastic="0"
     />
	 
	<scene name="eyvan" title="object" thumburl="panos/eyvan/thumb.jpg" onstart="action(startscene);">
		<preview url="panos/eyvan/preview.jpg" />
		<image >
			<cube url="panos/eyvan/mobile_%s.jpg" />
		</image>
	</scene>

</krpano>